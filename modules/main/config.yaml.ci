server:
  host: ${M_PERMISSION_HOST}
  port: ${M_PERMISSION_PORT}
  route: "/api/department"

external: true

features:
  enabled: []

database:
  provider: ${DB_PROVIDER}
  automigrate: true
  postgres:
    host: ${PG_HOST}
    port: ${PG_PORT}
    user: ${PG_USER}
    password: ${PG_PASSWORD}
    name: ${PG_DBNAME}
    sslmode: ${PG_SSL}